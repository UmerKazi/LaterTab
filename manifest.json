{
  "name": "LaterTab",
  "description": "Save your currently open tabs to view them at a later time",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_title": "Saved tabs",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
